page {
  height: 100%;
}
.cate_page {
  height: 100%;
}
.cate_page .All_content {
  height: calc( 100vh - 90rpx );
  display: flex;
}
.cate_page .All_content .left_menu {
  flex: 2;
  border-right: 1px solid #ccc;
}
.cate_page .All_content .left_menu .left_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cate_page .All_content .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cate_page .All_content .right_content {
  flex: 5;
}
.cate_page .All_content .right_content .title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cate_page .All_content .right_content .title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cate_page .All_content .right_content .list_container {
  display: flex;
  flex-wrap: wrap;
}
.cate_page .All_content .right_content .list_container navigator {
  width: 33.33%;
  text-align: center;
}
.cate_page .All_content .right_content .list_container navigator image {
  width: 100%;
}
